{
  "name": "custom-d/sso",
  "description": "A sso",
  "license": "MIT",
  "keywords": [
    "sso"
  ],
  "type": "library",
  "authors": [
    {
      "name": "Craig Smith",
      "email": "craig.smith@customd.com"
    }
  ],
  "require": {
    "php": ">=7.4",
    "illuminate/support": "^7.0|^8.0",
    "laravel/socialite": "^5.2",
    "spatie/laravel-package-tools": "^1.9"
  },
  "require-dev": {
    "orchestra/testbench": "^5.0|^6.0",
    "phpunit/phpunit": "^9.0|^10.0"
  },
  "autoload": {
    "psr-4": {
      "CustomD\\Sso\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "CustomD\\Sso\\Tests\\": "tests"
    }
  },
  "scripts": {
    "phpunit": "phpunit"
  },
  "extra": {
    "laravel": {
      "providers": [
        "CustomD\\Sso\\ServiceProvider"
      ]
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
